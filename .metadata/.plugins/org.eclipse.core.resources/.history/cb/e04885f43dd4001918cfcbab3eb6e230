    pageEncoding="UTF-8"%>
<%@ page import = "model.UserDataBean" %>	<%-- 소스에 있는 자바파일 임포트하는 방법 --%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Result</title>
</head>
<body>
	<h1>회원가입 결과 출력</h1>
	<%
		UserDataBean user2 = (UserDataBean)request.getAttribute("user");	// Attribute로 넘겨받은 값은 스트링(?)이므로 유저데이터빈 자료형으로 바꿔줌
	%>
	
	FirstName : <%= user2.getFirstname() %>
	lastName : <%= user2.getLastname() %>
	email : <%= user2.getEmail()() %>
</body>
